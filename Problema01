#include <stdio.h>

int main() {
    int num, div1, div2, div3, div4, div5;

    printf("Digite um número de 5 dígitos: ");
    scanf("%d", &num);

    div1 = num / 10000;
    div2 = (num / 1000) % 10;
    div3 = (num / 100) % 10;
    div4 = (num / 10) % 10;
    div5 = num % 10;

    printf("%d   %d   %d   %d   %d\n", div1, div2, div3, div4, div5);
    return 0;
}
